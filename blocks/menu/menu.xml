<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
    <div class="menu">
        <ul class="menu__list">
            <fest:each iterate="json.options" index="i" value="item">
                <li class="menu__item">
                    <a href="{item.href}">
                        <fest:value>item.title</fest:value>
                    </a>
                </li>
            </fest:each>
        </ul>
    </div>
</fest:template>